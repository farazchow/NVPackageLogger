{
  "name": "nvpackagelogging",
  "version": "1.0.0",
  "description": "New Vassar Package Logging System",
  "scripts": {
    "server": "npx ts-node server/server.tsx",
    "start": "webpack serve --config webpack.dev.config.ts",
    "dev": "concurrently --kill-others-on-fail \"npm run start\" \"npm run server\"",
    "stop": "npx kill-port 3000 && npx kill-port 4000",
    "cyclic-dependency": "npx madge --circular --extensions ts ./",
    "test": "echo create the first one!",
    "build": "webpack --progress --config webpack.prod.config.ts",
    "lint": "eslint --ext .ts,.tsx client",
    "format": "prettier --write --check client"
  },
  "author": "NV Deskworkers",
  "license": "MIT",
